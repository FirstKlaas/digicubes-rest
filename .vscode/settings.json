{
    "python.pythonPath": "${workspaceFolder}/venv/bin/python",
    "restructuredtext.confPath": "${workspaceFolder}/source",
    "restructuredtext.builtDocumentationPath": "${workspaceFolder}/build",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": {"when": "$(basename).py"},
        "**/__pycache__": true
    }
}